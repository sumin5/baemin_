계정은 baemin
비번 1234 생성
DB는 마리아

CREATE TABLE `tbl_member` (
	`userid` VARCHAR(20) NOT NULL,
	`userpassword` VARCHAR(300) NULL DEFAULT NULL,
	`username` VARCHAR(300) NULL DEFAULT NULL,
	`telno` VARCHAR(300) NULL DEFAULT NULL,
	`email` VARCHAR(300) NULL DEFAULT NULL,
	`address` VARCHAR(300) NULL DEFAULT NULL,
	`reg_date` TIMESTAMP NULL DEFAULT current_timestamp(),
	`mod_date` TIMESTAMP NOT NULL DEFAULT current_timestamp() ON UPDATE current_timestamp(),
	`authority_code` CHAR(2) NULL DEFAULT '0',
	PRIMARY KEY (`userid`) USING BTREE
);

tbl_member_detailCREATE TABLE `tbl_member_detail` (
	`userid` VARCHAR(20) NOT NULL,
	`bank_name` VARCHAR(30) NULL DEFAULT NULL,
	`bank_number` VARCHAR(30) NULL DEFAULT NULL,
	`agree` VARCHAR(10) NULL DEFAULT NULL,
	`couponid` VARCHAR(20) NULL DEFAULT NULL,
	`delivery_zipcode` VARCHAR(8) NULL DEFAULT NULL,
	`delivery_road` VARCHAR(40) NULL DEFAULT NULL,
	`delivery_detail` VARCHAR(40) NULL DEFAULT NULL,
	`address_code_group_id` VARCHAR(8) NULL DEFAULT NULL,
	`address_code_id` VARCHAR(8) NULL DEFAULT NULL,
	`member_state_id` VARCHAR(8) NULL DEFAULT NULL,
	`reg_date` TIMESTAMP NULL DEFAULT current_timestamp(),
	`mod_date` TIMESTAMP NOT NULL DEFAULT current_timestamp() ON UPDATE current_timestamp(),
	PRIMARY KEY (`userid`) USING BTREE,
	FOREIGN KEY(`userid`) REFERENCES tbl_member(`userid`)
	ON DELETE CASCADE
);
